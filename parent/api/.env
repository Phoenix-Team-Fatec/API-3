# Configurações do banco de dados PostgreSQL
DB_NAME=oak     # Nome do banco de dados
DB_USER=postgres          # Usuário do banco
DB_PASSWORD=1234   # Senha do banco
DB_HOST=localhost         # Host do banco de dados (geralmente localhost em dev)
DB_PORT=5432              # Porta padrão do PostgreSQL

# Configuração do JWT
JWT_SECRET=my-secret-key
  # Chave secreta para geração de tokens JWT

# Porta do servidor
PORT=3000                            # Porta em que o servidor Express vai rodar

FIREBASE_API_KEY=your_api_key
FIREBASE_AUTH_DOMAIN=your_auth_domain
FIREBASE_PROJECT_ID=your_project_id
FIREBASE_STORAGE_BUCKET=your_storage_bucket
FIREBASE_MESSAGING_SENDER_ID=your_sender_id
FIREBASE_APP_ID=your_app_id